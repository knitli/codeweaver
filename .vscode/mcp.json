{
    "inputs": [
        {
            "description": "GitHub Personal Access Token",
            "id": "github_mcp_pat",
            "password": true,
            "type": "promptString"
        },
        {
            "description": "Tavily API Key",
            "id": "tavily_api_key",
            "password": true,
            "type": "promptString"
        }
    ],
    "servers": {
        "browser-debug-tools": {
            "args": [
                "--bun",
                "@agentdeskai/browser-tools-server@latest"
            ],
            "command": "bunx",
            "type": "stdio"
        },
        "context7": {
            "args": [
                "--bun",
                "@upstash/context7-mcp@latest"
            ],
            "command": "bunx",
            "type": "stdio"
        },
        "fetch": {
            "args": [
                "mcp-server-fetch"
            ],
            "command": "uvx"
        },
        "sequential-thinking": {
            "args": [
                "--bun",
                "@modelcontextprotocol/server-sequential-thinking"
            ],
            "command": "bunx",
            "type": "stdio"
        },
        "tavily": {
            "command": "bunx",
            "args": [
                "--bun",
                "tavily-mcp@latest"
            ],
            "env": {
                "TAVILY_API_KEY": "${env:tavily_api_key}",
            }
        },
        "serena": {
            "command": "uv",
            "args": [
                "run",
                "serena",
                "start-mcp-server"
            ]
        },
        "morph-llm-fast-edit": {
            "args": [
                "--bun",
                "@morph-llm/morph-fast-apply@latest"
            ],
            "env": {
                "MORPH_API_KEY": "${env:MORPH_API_KEY}",
                "ALL_TOOLS": "false"
            },
            "command": "bunx",
            "type": "stdio"
        },
    }
}