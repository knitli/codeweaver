{
  "servers": {
    "context7": {
      "args": [
        "--bun",
        "@upstash/context7-mcp@latest"
      ],
      "command": "bunx",
      "type": "stdio"
    },
    "morphLlmFastApply": {
      "args": [
        "--bun",
        "@morph-llm/morph-fast-apply@latest"
      ],
      "command": "bunx",
      "env": {
        "ALL_TOOLS": "false",
        "MORPH_API_KEY": "${env:MORPH_API_KEY}"
      },
      "type": "stdio"
    },
    "serena": {
      "args": [
        "run",
        "serena",
        "start-mcp-server",
        "--context",
        "agent"
      ],
      "command": "uv"
    },
    "tavily": {
      "args": [
        "--bun",
        "tavily-mcp@latest"
      ],
      "command": "bunx",
      "env": {
        "TAVILY_API_KEY": "${env:TAVILY_API_KEY}"
      }
    }
  }
}