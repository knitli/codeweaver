{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "MTEB Conversion Script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/model-data/mteb-to-codeweaver.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "stopOnEntry": true
    },
    {
      "name": "CLI",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/codeweaver/cli/__main__.py",
      "args": "${command:pickArgs}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Module as-script",
      "type": "debugpy",
      "program": "${file}",
      "request": "launch",
      "console": "integratedTerminal",
      "justMyCode": true,
      "stopOnEntry": true,
      "args": "${command:pickArgs}"
    },
    {
      "name": "Selected-Text",
      "type": "debugpy",
      "request": "launch",
      "program": "${selectedText}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "stopOnEntry": true,
      "args": "${command:pickArgs}",
      "showReturnValue": true
    },
    {
      "name": "Get-Langs Script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/language-support/get-langs.py",
      "console": "integratedTerminal"
    }
  ]
}
