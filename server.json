{
  "_meta": {
    "io.modelcontextprotocol.registry/publisher-provided": {
      "build_info": {
        "framework": "fastmcp",
        "package_manager": "uv",
        "python_version": ">=3.12"
      },
      "capabilities": {
        "chunking_strategies": [
          "semantic",
          "semantically-aware-delimiters"
        ],
        "embedding_providers": [
          "voyage",
          "cohere",
          "openai",
          "mistral",
          "google",
          "huggingface",
          "fastembed",
          "sentence_transformers",
          "bedrock"
        ],
        "languages_supported": 170,
        "reranking_providers": [
          "voyage",
          "cohere",
          "openai",
          "mistral",
          "google",
          "huggingface"
        ],
        "search_types": [
          "semantic",
          "hybrid",
          "keyword"
        ],
        "sparse_embedding_providers": [
          "fastembed",
          "sentence_transformers"
        ],
        "vector_stores": [
          "qdrant",
          "inmemory"
        ]
      },
      "tags": [
        "agent-tools",
        "ast-parsing",
        "code-search",
        "code-understanding",
        "developer-tools",
        "embeddings",
        "hybrid-search",
        "multi-language",
        "natural-language-processing",
        "reranking",
        "semantic-search",
        "sparse-embeddings",
        "vector-database"
      ]
    }
  },
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-09-16/server.schema.json",
  "description": "Semantic code search built for AI agents. Hybrid AST-aware embeddings for 170+ languages.",
  "name": "io.github.knitli/codeweaver",
  "packages": [
    {
      "environmentVariables": [
        {
          "description": "Project path (alternative to --project argument)",
          "isRequired": false,
          "name": "CODEWEAVER_PROJECT_PATH"
        },
        {
          "description": "Configuration file path (alternative to --config argument)",
          "isRequired": false,
          "name": "CODEWEAVER_CONFIG_FILE"
        },
        {
          "default": "voyage",
          "description": "Embedding provider to use (voyage, cohere, openai, fastembed, sentence_transformers, etc.)",
          "isRequired": false,
          "name": "CODEWEAVER_EMBEDDING_PROVIDER"
        },
        {
          "default": "voyage-code-3",
          "description": "Specific embedding model to use (provider-dependent)",
          "isRequired": false,
          "name": "CODEWEAVER_EMBEDDING_MODEL"
        },
        {
          "default": "fastembed",
          "description": "Sparse embedding provider to use for hybrid search (fastembed, sentence_transformers)",
          "isRequired": false,
          "name": "CODEWEAVER_SPARSE_EMBEDDING_PROVIDER"
        },
        {
          "default": "prithivida/Splade_pp_en_v1",
          "description": "Specific sparse embedding model to use (provider-dependent)",
          "isRequired": false,
          "name": "CODEWEAVER_SPARSE_EMBEDDING_MODEL"
        },
        {
          "default": "qdrant",
          "description": "Vector store backend (qdrant, memory)",
          "isRequired": false,
          "name": "CODEWEAVER_VECTOR_STORE"
        },
        {
          "description": "API key for Voyage AI embedding provider. Required with default/recommended 'voyage' provider.",
          "isRequired": false,
          "isSecret": true,
          "name": "VOYAGE_API_KEY"
        },
        {
          "description": "API key for Cohere embedding provider",
          "isRequired": false,
          "isSecret": true,
          "name": "COHERE_API_KEY"
        },
        {
          "description": "API key for OpenAI embedding provider",
          "isRequired": false,
          "isSecret": true,
          "name": "OPENAI_API_KEY"
        },
        {
          "description": "API key for Mistral AI embedding provider",
          "isRequired": false,
          "isSecret": true,
          "name": "MISTRAL_API_KEY"
        },
        {
          "description": "API key for Google AI embedding provider",
          "isRequired": false,
          "isSecret": true,
          "name": "GOOGLE_API_KEY"
        },
        {
          "description": "API key for HuggingFace embedding provider",
          "isRequired": false,
          "isSecret": true,
          "name": "HUGGINGFACE_API_KEY"
        },
        {
          "description": "Qdrant server URL for remote vector store",
          "isRequired": false,
          "name": "QDRANT_URL"
        },
        {
          "description": "API key for Qdrant cloud/remote instances",
          "isRequired": false,
          "isSecret": true,
          "name": "QDRANT_API_KEY"
        },
        {
          "description": "Alternative variable for Qdrant API key",
          "isRequired": false,
          "isSecret": true,
          "name": "QDRANT__SERVICE__API_KEY"
        },
        {
          "default": "false",
          "description": "Enable/disable anonymous telemetry (true/false). We use telemetry *only* to improve the product.",
          "isRequired": false,
          "name": "CODEWEAVER__TELEMETRY__DISABLE_TELEMETRY"
        },
        {
          "default": "false",
          "description": "Optionally opt-in to share data on queries and responses to help improve embedding and search quality (true/false). Potentially identifying, though we try to minimize this.",
          "isRequired": false,
          "name": "CODEWEAVER__TELEMETRY__TOOLS_OVER_PRIVACY"
        }
      ],
      "identifier": "codeweaver",
      "packageArguments": [
        {
          "description": "Path to the code repository to index and search. Will attempt to detect if not provided.",
          "isRequired": false,
          "name": "--project",
          "type": "named",
          "valueHint": "path"
        },
        {
          "description": "Path to configuration file (TOML, YAML or JSON format)",
          "isRequired": false,
          "name": "--config",
          "type": "named",
          "valueHint": "file_path"
        },
        {
          "default": "127.0.0.1",
          "description": "Host address for MCP server",
          "isRequired": false,
          "name": "--host",
          "type": "named"
        },
        {
          "default": "9328",
          "description": "Port for MCP server",
          "format": "number",
          "isRequired": false,
          "name": "--port",
          "type": "named"
        },
        {
          "choices": [
            "debug",
            "info",
            "warning",
            "error",
            "critical"
          ],
          "default": "info",
          "description": "Logging level",
          "isRequired": false,
          "name": "--log-level",
          "type": "named"
        }
      ],
      "registryBaseUrl": "https://pypi.org",
      "registryType": "pypi",
      "runtimeHint": "uvx",
      "transport": {
        "type": "stdio"
      },
      "version": "0.0.1rc778"
    },
    {
      "environmentVariables": [
        {
          "default": "/workspace",
          "description": "Repository path inside container",
          "isRequired": true,
          "name": "CODEWEAVER_REPO_PATH"
        },
        {
          "default": "voyage",
          "description": "Embedding provider to use",
          "isRequired": false,
          "name": "CODEWEAVER_EMBEDDING_PROVIDER"
        },
        {
          "description": "API key for Voyage AI",
          "isRequired": false,
          "isSecret": true,
          "name": "VOYAGE_API_KEY"
        },
        {
          "description": "API key for Cohere",
          "isRequired": false,
          "isSecret": true,
          "name": "COHERE_API_KEY"
        },
        {
          "description": "API key for OpenAI",
          "isRequired": false,
          "isSecret": true,
          "name": "OPENAI_API_KEY"
        },
        {
          "default": "http://localhost:6333",
          "description": "Qdrant server URL (e.g., http://qdrant:6333)",
          "isRequired": false,
          "name": "QDRANT_URL"
        },
        {
          "description": "API key for Qdrant cloud",
          "isRequired": false,
          "isSecret": true,
          "name": "QDRANT_API_KEY"
        },
        {
          "default": "0.0.0.0",
          "description": "Host address for MCP server",
          "isRequired": false,
          "name": "CODEWEAVER_HOST"
        },
        {
          "default": "9328",
          "description": "Port for MCP server",
          "isRequired": false,
          "name": "CODEWEAVER_PORT"
        },
        {
          "default": "info",
          "description": "Logging verbosity",
          "isRequired": false,
          "name": "LOG_LEVEL"
        },
        {
          "default": "false",
          "description": "Enable/disable telemetry",
          "isRequired": false,
          "name": "CODEWEAVER_TELEMETRY_ENABLED"
        }
      ],
      "identifier": "knitli/codeweaver",
      "packageArguments": [
        {
          "description": "Start CodeWeaver MCP server",
          "type": "positional",
          "value": "server"
        },
        {
          "description": "Bind to all interfaces in container",
          "name": "--host",
          "type": "named",
          "value": "0.0.0.0"
        },
        {
          "description": "MCP server port inside container",
          "name": "--port",
          "type": "named",
          "value": "9328"
        }
      ],
      "registryBaseUrl": "https://docker.io",
      "registryType": "oci",
      "runtimeArguments": [
        {
          "description": "Automatically remove container when it exits",
          "name": "--rm",
          "type": "named"
        },
        {
          "description": "Mount workspace directory as read-only",
          "name": "-v",
          "type": "named",
          "value": "{workspace}:/workspace:ro",
          "variables": {
            "workspace": {
              "description": "Path to your codebase to index and search",
              "isRequired": true
            }
          }
        },
        {
          "description": "Set repository path inside container",
          "name": "-e",
          "type": "named",
          "value": "CODEWEAVER_PROJECT_PATH=/workspace"
        },
        {
          "description": "Voyage AI API key for embeddings",
          "name": "-e",
          "type": "named",
          "value": "VOYAGE_API_KEY={voyage_api_key}",
          "variables": {
            "voyage_api_key": {
              "description": "Your Voyage AI API key",
              "isRequired": false
            }
          }
        },
        {
          "description": "Embedding provider selection",
          "name": "-e",
          "type": "named",
          "value": "CODEWEAVER_EMBEDDING_PROVIDER={embedding_provider}",
          "variables": {
            "embedding_provider": {
              "choices": [
                "voyage",
                "cohere",
                "openai",
                "fastembed",
                "sentence_transformers"
              ],
              "default": "voyage",
              "description": "Provider to use for embeddings"
            }
          }
        },
        {
          "description": "Port mapping for MCP server",
          "name": "-p",
          "type": "named",
          "value": "{host_port}:9328",
          "variables": {
            "host_port": {
              "default": "9328",
              "description": "Host port to expose MCP server"
            }
          }
        },
        {
          "description": "Docker network for Qdrant connectivity",
          "name": "--network",
          "type": "named",
          "value": "{network}",
          "variables": {
            "network": {
              "default": "bridge",
              "description": "Docker network name (use 'host' for local Qdrant or custom network)"
            }
          }
        }
      ],
      "runtimeHint": "docker",
      "transport": {
        "type": "stdio"
      },
      "version": "0.0.1rc778"
    }
  ],
  "repository": {
    "source": "github",
    "url": "https://github.com/knitli/codeweaver"
  },
  "status": "active",
  "title": "CodeWeaver - Semantic Code Search",
  "version": "0.0.1rc778",
  "websiteUrl": "https://github.com/knitli/codeweaver"
}